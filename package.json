    "psn-api": "^2.11.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.33.5",
    "steamapi": "^3.0.12",
    "supertokens-node": "^22.0.1",
    "ts-pattern": "^5.6.2",
    "typeorm": "^0.3.21",
    "unique-username-generator": "^1.4.0"
