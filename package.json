{
  "name": "game-node-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "type": "commonjs",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=2048 dist/src/main",
    "start:dev:docker": "docker compose up --build -d && yarn start:dev",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "yarn run typeorm migration:create",
    "migration:generate": "yarn run typeorm migration:generate ./src/migrations/all -d ./data-source.ts",
    "migration:run": "yarn run typeorm migration:run -d ./data-source.ts",
    "healthcheck": "node src/health/healthcheck.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis-health": "^9.0.4",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bullmq": "^10.2.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.7",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.7",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/async-retry": "^1.4.6",
    "@types/bad-words": "^3.0.3",
    "@types/multer": "^1.4.7",
    "async-retry": "^1.3.3",
    "axios": "^1.7.7",
    "bad-words": "^3.0.4",
    "bullmq": "^5.26.0",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.13",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mime-types": "^2.1.35",
    "mysql2": "^3.11.4",
    "nestjs-throttler-storage-redis": "^0.5.1",
    "psn-api": "^2.11.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "steamapi": "^3.0.8",
    "supertokens-node": "^20.0.0",
    "ts-pattern": "^5.6.2",
    "typeorm": "^0.3.20",
    "unique-username-generator": "^1.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.3.7",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/igdb-api-node": "^5.0.0",
    "@types/jest": "29.5.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "@types/user-agents": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.7.0",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
